   pod 'Masonry'
  # Pods for FleetingTime

  target 'FleetingTimeTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'FleetingTimeUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
